---
export const prerender = true;
import Layout from "../layouts/Layout.astro";
import { PortfolioPage } from "../components/landing/PortfolioPage";
import { Navbar } from "../components/landing/Navbar";
import { FloatingCTA } from "../components/landing/FloatingCTA";
import Footer from "../components/landing/Footer.astro";

// SEO Configuration for Portfolio
const title =
  "Software Development Portfolio | 150+ Successful Projects | OpenOrbit";
const description =
  "Explore OpenOrbit's high-impact software development portfolio. 150+ successful projects including enterprise web apps, mobile applications, AI/ML solutions, cloud platforms, and digital transformation initiatives. View case studies from Fortune 500 companies and innovative startups.";

const keywords = [
  "software development portfolio",
  "web application case studies",
  "mobile app portfolio",
  "enterprise software projects",
  "AI development projects",
  "cloud platform case studies",
  "digital transformation examples",
  "React project portfolio",
  "fintech software development",
  "healthcare app development",
];
---

<Layout title={title} description={description} keywords={keywords}>
  <div
    class="relative min-h-screen bg-[#070A12] text-[#F4F6FF]"
    itemscope
    itemtype="https://schema.org/CollectionPage"
    role="none"
  >
    <Navbar client:load />
    <PortfolioPage client:load />
    <Footer />
    <FloatingCTA client:visible />
  </div>
</Layout>
